  "scripts": {
    "v..": "npm version patch --no-git-tag-version",
    "v.": "npm version minor --no-git-tag-version",
    "v": "npm version major --no-git-tag-version",
    "clean:all": "rm -rf node_modules build package-lock.json",
    "clean:prod": "rm -rf node_modules package-lock.json",
    "preupdate": "npm run npmconf",
    "update": "npx npm-check-updates -u",
    "postupdate": "npm i",
    "build": "react-scripts build",
    "start": "npm run dev:fullclean",
    "prenpmconf": "rm -f .npmrc",
    "npmconf": "aws codeartifact login --tool npm --repository failean --domain failean --domain-owner 988253048728 --region us-east-1 && npm config set registry https://registry.npmjs.org/ && npm config set @failean:registry https://failean-988253048728.d.codeartifact.us-east-1.amazonaws.com/npm/failean/",
    "prod": "npm run update && npm run build",
    "docker": "VERSION=$(node -p \"require('./package.json').version\") && docker build -t 988253048728.dkr.ecr.us-east-1.amazonaws.com/client:$VERSION .",
    "dev:fullclean": "npm run clean:all && npm run dev:fullfast",
    "dev:fullfast": "npm run update && npm run dev:fast",
    "dev:fast": "PORT=5999 react-scripts start",
    "dev": "npm run dev:fast",
    "udev": "npm run dev:fullfast",
    "eject": "react-scripts eject",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
