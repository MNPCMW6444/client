{
  "name": "failean-client",
  "version": "7.0.0",
  "private": true,
  "proxy": "http://localhost:5920",
  "scripts": {
    "v..": "npm version patch --no-git-tag-version",
    "v.": "npm version minor --no-git-tag-version",
    "v": "npm version major --no-git-tag-version",
    "clean:all": "rm -rf node_modules build package-lock.json",
    "clean:prod": "rm -rf node_modules package-lock.json",
    "update": "npx npm-check-updates -u && npm i",
    "update:prod": "npx npm-check-updates -u && npm i",
    "build": "react-scripts build",
    "start": "npm run dev:fullclean",
    "prod": "npm run update:prod && npm run build",
    "docker": "VERSION=$(node -p \"require('./package.json').version\") docker build --build-arg -t client:$VERSION .",
    "dev:fullclean": "npm run clean:all && npm run dev:fullfast",
    "dev:fullfast": "npm run update && npm run dev:fast",
    "server": "node server.js",
    "dev:fast": "concurrently \"npm run server\" \"PORT=5999 react-scripts start\"",
    "dev": "npm run dev:fast",
    "udev": "npm run dev:fullfast",
    "eject": "react-scripts eject",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.8.2",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@failean/mainserver-provider": "^2.7.122",
    "@failean/shared-types": "^2.9.161",
    "@fontsource-variable/montserrat": "^5.0.9",
    "@mui/icons-material": "^5.14.7",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash": "^4.14.197",
    "@types/zxcvbn": "^4.4.2",
    "concurrently": "^8.2.1",
    "graphql": "^16.8.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.15.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.1.3",
    "web-vitals": "^3.4.0",
    "zxcvbn": "^4.4.2"
  }
}
