{
  "name": "failean-client",
  "version": "4.1.0",
  "private": true,
  "devDependencies": {
    "@apollo/client": "^3.7.17",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@failean/mainserver-provider": "^2.7.110",
    "@failean/shared-types": "^2.9.159",
    "@fontsource-variable/montserrat": "^5.0.6",
    "@mui/icons-material": "^5.14.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash": "^4.14.196",
    "@types/zxcvbn": "^4.4.1",
    "graphql": "^16.7.1",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.1.3",
    "web-vitals": "^3.4.0",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "preclean": "npm run connect",
    "clean": "rm -rf node_modules build package-lock.json",
    "clean:p": "rm -rf node_modules package-lock.json",
    "preupdatesimple": "npx npm-check-updates -u",
    "updatesimple": "npm i",
    "preupdate": "npm run clean",
    "update": "npm run updatesimple",
    "prebuild": "npm run version:patch",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "start": "npm run udev",
    "preprod": "npm run update",
    "preconect": "aws codeartifact login --tool npm --repository failean --domain failean --domain-owner 988253048728 --region us-east-1 && echo '@failean:registry=https://failean-988253048728.d.codeartifact.us-east-1.amazonaws.com/npm/failean/' >> ./.npmrc",
    "connect": "npm run prod",
    "prod": "npm run build",
    "d:b": "VERSION=$(node -p \"require('./package.json').version\") && docker build --platform amd64 -t 988253048728.dkr.ecr.us-east-1.amazonaws.com/client:$VERSION .",
    "dev": "PORT=5999 react-scripts start",
    "preudev": "npm run update",
    "udev": "npm run dev",
    "predevwindows": "npm i",
    "devwindows": "export PORT=5999 &react-scripts start",
    "pred:b": "rm -f .npmrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
